@model BoardGame1.Models.MembershipDto

@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete this Membership Account?</h2>
<a href="/Membership/List">Cancel</a>
<h3>ID: @Model.MembershipId</h3>
<div>First Name: @Model.FirstName</div>
<div>Last Name: @Model.LastName</div>
<form action="/Membership/Delete/@Model.MembershipId" method="POST">
    <input type="submit" value="Confirm Delete" />
</form>
